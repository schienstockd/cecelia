{
  "fun": {
    "cellpose": {
      "category": "Module functions",
      "label": "Cellpose workflow",
      "env": ["local", "hpc"]
    }
  },
  "spec": {
    "inputs": {
      "valueName": {
				"type:character": {
					"default": "default"
				}
			},
      "models": {
        "type:group": {
          "model": {
            "type:character": {
              "default": "cyto3"
            }
          },
          "deblur_cyto3": {
            "type:character": {
              "default": "cyto3"
            }
          },
          "matchAs": {
            "type:character": {
              "default": "none"
            }
          },
          "cellDiameter": {
            "type:double": {
              "min": 1,
              "max": 50,
              "default": 10
            }
          },
          "cellChannels": {
            "type:list": {}
          },
          "nucChannels": {
            "type:list": {}
          },
					"normalise": {
						"type:double": {
							"min": 80,
							"max": 100,
							"default": 99.8
						}
					},
					"box1": {
            "type:box": {
    					"stitchThreshold": {
    						"type:double": {
    							"min": 0,
    							"max": 1,
    							"default": 0.2
    						}
    					},
    					"mergeLabels": {
    						"type:boolean": {
    							"default": false
    						}
    					},
    					"threshold": {
    						"type:integer": {
    							"min": 0,
    							"max": 1000,
    							"default": 0
    						}
    					},
    					"relThreshold": {
    						"type:double": {
    							"min": 0,
    							"max": 100,
    							"default": 0
    						}
    					},
    					"medianFilter": {
    						"type:integer": {
    							"min": 0,
    							"max": 10,
    							"default": 0
    						}
    					},
    					"gaussianFilter": {
    						"type:integer": {
    							"min": 0,
    							"max": 10,
    							"default": 0
    						}
    					},
    					"sumFilter": {
    						"type:integer": {
    							"min": 0,
    							"max": 10,
    							"default": 0
    						}
    					}
            }
					}
        }
      },
  		"box2": {
        "type:box": {
          "matchThreshold": {
      			"type:double": {
      				"min": 0,
      				"max": 1,
      				"default": 0.05
      			}
    			},
    			"removeUnmatched": {
      			"type:boolean": {
      				"default": true
      			}
    			},
    			"minCellSize": {
    				"type:integer": {
    					"min": 0,
    					"max": 1000,
    					"default": 100
    				}
    			},
    			"labelExpansion": {
    				"type:integer": {
    					"min": 0,
    					"max": 10,
    					"default": 0
    				}
    			},
    			"labelErosion": {
    				"type:integer": {
    					"min": 0,
    					"max": 10,
    					"default": 0
    				}
    			},
    			"haloSize": {
    				"type:integer": {
    					"min": 0,
    					"max": 20,
    					"default": 0
    				}
    			},
    			"haloWholeCell": {
    				"type:boolean": {
    					"default": false
    				}
    			}
        }
  		},
  		"box3": {
        "type:box": {
    			"blockSize": {
    				"type:integer": {
    					"min": -1,
    					"max": 2048,
    					"default": 2048
    				}
    			},
    			"overlap": {
    				"type:integer": {
    					"min": -1,
    					"max": 256,
    					"default": 64
    				}
    			},
    			"blockSizeZ": {
    				"type:integer": {
    					"min": -1,
    					"max": 1024,
    					"default": -1
    				}
    			},
    			"overlapZ": {
    				"type:integer": {
    					"min": -1,
    					"max": 64,
    					"default": -1
    				}
    			},
    			"context": {
    				"type:integer": {
    					"min": 1,
    					"max": 128,
    					"default": 52
    				}
    			},
    			"labelOverlap": {
    				"type:double": {
    					"min": 0,
    					"max": 1,
    					"default": 0.2
    				}
    			},
    			"clearDepth": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"clearTouchingBorder": {
    				"type:boolean": {
    					"default": false
    				}
    			}
        }
  		},
			"box4": {
        "type:box": {
          "segment": {
    				"type:boolean": {
    					"default": true
    				}
    			},
    			"useDask": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"measure": {
    				"type:boolean": {
    					"default": true
    				}
    			},
    			"calcMedianIntensities": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"updateMeasures": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"saveMeasures": {
    				"type:boolean": {
    					"default": true
    				}
    			},
    			"saveMeshes": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"extendedMeasures": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"integrateTime": {
    				"type:boolean": {
    					"default": false
    				}
    			},
    			"integrateTimeMode": {
            "type:character": {
              "default": "max"
            }
    			},
    			"normaliseToWhole": {
    				"type:boolean": {
    					"default": true
    				}
    			}
        }
			}
    },
    "outputs": {}
  },
