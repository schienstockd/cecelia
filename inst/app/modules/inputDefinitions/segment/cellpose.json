{
  "fun": {
    "cellpose": {
      "category": "Module functions",
      "label": "Cellpose workflow",
      "env": ["local", "hpc"]
    }
  },
  "spec": {
    "inputs": {
      "valueName": {
				"type:character": {
					"default": "default"
				}
			},
      "models": {
        "type:group": {
          "model": {
            "type:character": {
              "default": "cyto2"
            }
          },
          "matchAs": {
            "type:character": {
              "default": "none"
            }
          },
          "cellDiameter": {
            "type:double": {
              "min": 1,
              "max": 50,
              "default": 10
            }
          },
          "cellChannels": {
            "type:list": {}
          },
          "nucChannels": {
            "type:list": {}
          },
					"normalise": {
						"type:double": {
							"min": 80,
							"max": 100,
							"default": 99.9
						}
					},
					"stitchThreshold": {
						"type:double": {
							"min": 0,
							"max": 1,
							"default": 0.0
						}
					},
					"mergeLabels": {
						"type:boolean": {
							"default": false
						}
					},
					"threshold": {
						"type:integer": {
							"min": 0,
							"max": 1000,
							"default": 0
						}
					},
					"relThreshold": {
						"type:double": {
							"min": 0,
							"max": 100,
							"default": 0
						}
					},
					"medianFilter": {
						"type:integer": {
							"min": 0,
							"max": 10,
							"default": 0
						}
					},
					"gaussianFilter": {
						"type:integer": {
							"min": 0,
							"max": 10,
							"default": 0
						}
					}
        }
      },
			"minCellSize": {
				"type:integer": {
					"min": 0,
					"max": 1000,
					"default": 10
				}
			},
			"labelExpansion": {
				"type:integer": {
					"min": 0,
					"max": 10,
					"default": 0
				}
			},
			"labelErosion": {
				"type:integer": {
					"min": 0,
					"max": 10,
					"default": 0
				}
			},
			"haloSize": {
				"type:integer": {
					"min": 0,
					"max": 20,
					"default": 0
				}
			},
			"haloWholeCell": {
				"type:boolean": {
					"default": false
				}
			},
			"blockSize": {
				"type:integer": {
					"min": -1,
					"max": 2048,
					"default": 512
				}
			},
			"overlap": {
				"type:integer": {
					"min": -1,
					"max": 256,
					"default": 64
				}
			},
			"blockSizeZ": {
				"type:integer": {
					"min": -1,
					"max": 1024,
					"default": 100
				}
			},
			"overlapZ": {
				"type:integer": {
					"min": -1,
					"max": 64,
					"default": 10
				}
			},
			"context": {
				"type:integer": {
					"min": 1,
					"max": 128,
					"default": 52
				}
			},
			"clearDepth": {
				"type:boolean": {
					"default": false
				}
			},
			"clearTouchingBorder": {
				"type:boolean": {
					"default": false
				}
			},
			"segment": {
				"type:boolean": {
					"default": true
				}
			},
			"updateMeasures": {
				"type:boolean": {
					"default": false
				}
			},
			"saveMeasures": {
				"type:boolean": {
					"default": true
				}
			},
			"saveMeshes": {
				"type:boolean": {
					"default": false
				}
			},
			"extendedMeasures": {
				"type:boolean": {
					"default": false
				}
			}
    },
    "outputs": {}
  },
  "ui": {
