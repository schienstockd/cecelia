{
  "fun": {
    "seedDetectPreview": {
      "category": "Preview functions",
      "label": "Seed Detection",
      "env": ["local"]
    }
  },
  "spec": {
    "inputs": {
      "seedChannel": {
        "type:list": {
        }
      },
      "seedThresholdRel": {
        "type:double": {
          "min": 0,
          "max": 1,
          "default": 0
        }
      },
      "seedThresholdAbs": {
        "type:integer": {
          "default": 0
        }
      },
      "cellRadius": {
        "type:double": {
          "min": 0,
          "max": 20,
          "default": 4
        }
      },
      "zSpread": {
        "type:double": {
          "min": 1,
          "max": 10,
          "default": 1
        }
      },
      "cellMinDistance": {
        "type:double": {
          "min": 0,
          "max": 50,
          "default": 1
        }
      },
      "timepoints": {
        "type:integer": {
        }
      }
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
      "seedChannel": {
        "index": 0,
        "label": "Seed channel",
        "widget:channelSelection": {
          "size": 5,
          "multiple": false
        }
      },
      "seedThresholdRel": {
        "index": 1,
        "label": "Relative Threshold",
        "widget:slider": {
          "step": 0.01
        }
      },
      "seedThresholdAbs": {
        "index": 2,
        "label": "Absolute Threshold",
        "widget:sliderImageIntensity": {
          "step": 1
        }
      },
      "cellRadius": {
        "index": 3,
        "label": "Cell radius",
        "widget:slider": {
          "step": 0.1
        }
      },
      "zSpread": {
        "index": 4,
        "label": "Estimated Z spread",
        "widget:slider": {
          "step": 1
        }
      },
      "cellMinDistance": {
        "index": 5,
        "label": "Minimum cell distance",
        "widget:slider": {
          "step": 1
        }
      },
      "timepoints": {
        "index": 6,
        "label": "Timepoints",
        "widget:sliderTimepoints": {
          "step": 1,
          "range": true
        }
      }
    },
    "outputs": {}
  }
}
