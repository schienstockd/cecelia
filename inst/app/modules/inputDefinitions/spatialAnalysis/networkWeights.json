{
  "fun": {
    "networkWeights": {
      "category": "Module functions",
      "label": "Network weights",
      "env": ["local", "hpc"]
    }
  },
  "spec": {
    "inputs": {
			"valueName": {
				"type:character": {
					"default": "default"
				}
			},
			"pops": {
        "type:list": {}
      },
      "maxDist": {
        "type:double": {
          "min": 0,
          "max": 50,
          "default": 10
        }
      }
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
			"valueName": {
				"index": 0,
				"label": "Branching",
				"widget:valueNameSelection": {
					"size": 1,
					"multiple": false,
					"field": "imLabelsFilepath"
				}
			},
			"pops": {
        "index": 1,
        "label": "Populations",
        "widget:popSelection": {
          "size": 1,
          "multiple": true
        }
      },
      "maxDist": {
        "index": 2,
        "label": "Maximum distance",
        "widget:slider": {
          "step": 0.5
        }
      }
    },
    "outputs": {}
  }
}
