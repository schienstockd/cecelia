{
  "fun": {
    "distLeadingEdge": {
      "category": "Module functions",
      "label": "Distance leading edge",
      "env": ["local"]
    }
  },
  "spec": {
    "inputs": {
      "borderCutoff": {
        "type:double": {
          "min": 0,
          "max": 50,
          "default": 10
        }
      },
      "concavity": {
        "type:integer": {
          "min": 0,
          "max": 50,
          "default": 10
        }
      },
      "popsForEdge": {
        "type:list": {}
      }
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
      "borderCutoff": {
        "index": 0,
        "label": "Border cutoff",
        "widget:slider": {
          "step": 0.5
        }
      },
      "concavity": {
        "index": 1,
        "label": "Concavity",
        "widget:slider": {
          "step": 1
        }
      },
      "popsForEdge": {
        "index": 2,
        "label": "Populations",
        "widget:popSelection": {
          "size": 1,
          "multiple": true
        }
      }
    },
    "outputs": {}
  }
}
