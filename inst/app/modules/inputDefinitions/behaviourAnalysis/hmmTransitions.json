{
  "fun": {
    "hmmTransitions": {
      "category": "Module functions",
      "label": "HMM Transitions",
      "env": ["local"]
    }
  },
  "spec": {
    "inputs": {
			"pops": {
				"type:list": {}
			},
			"colName": {
				"type:character": {
					"default": "default"
				}
			},
			"hmmStates": {
				"type:list": {}
			},
			"includeStart": {
				"type:boolean": {
					"default": false
				}
			},
			"includeSelfTransitions": {
				"type:boolean": {
