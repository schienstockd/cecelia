{
  "fun": {
    "upload": {
      "category": "Synchronise data",
      "label": "Upload raw images",
      "env": ["local"]
    }
  },
  "spec": {
    "inputs": {
      "remoteEnv": {
        "type:character": {
          "default": "hpc"
        }
      },
      "isSequence": {
				"type:boolean": {
					"default": false
				}
			},
      "specialType": {
				"type:character": {
					"default": "none"
				}
			}
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
      "remoteEnv": {
        "index": 0,
        "label": "Remote environment",
        "widget:selection": {
          "items": {
            "HPC": "hpc"
          },
          "multiple": false,
          "size": 1
        }
      },
      "isSequence": {
				"index": 1,
				"label": "Image sequence",
				"widget:checkbox": {
				}
			},
			"specialType": {
        "index": 2,
        "label": "Special type",
        "widget:selection": {
          "items": {
            "NONE": "none",
            "10x Xenium": "tenxXenium"
          },
          "multiple": false,
          "size": 1
        }
      }
    },
    "outputs": {}
  }
}
