{
  "fun": {
    "transferFrom": {
      "category": "Synchronise data",
      "label": "Transfer from server",
      "env": ["local"]
    }
  },
  "spec": {
    "inputs": {
      "remoteFrom": {
        "type:character": {
          "default": "smb"
        }
      },
      "remoteTo": {
        "type:character": {
          "default": "hpc"
        }
      },
      "isSequence": {
				"type:boolean": {
					"default": false
				}
			}
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
      "remoteFrom": {
        "index": 0,
        "label": "From",
        "widget:selection": {
          "items": {
            "Lab server": "smb"
          },
          "multiple": false,
          "size": 1
        }
      },
      "remoteTo": {
        "index": 0,
        "label": "To",
        "widget:selection": {
          "items": {
            "HPC": "hpc"
          },
          "multiple": false,
          "size": 1
        }
      },
      "isSequence": {
				"index": 2,
				"label": "Image sequence",
				"widget:checkbox": {
				}
			}
    },
    "outputs": {}
  }
}
