{
  "fun": {
    "tenxXenium": {
      "category": "Module functions",
      "label": "10X Xenium",
      "env": ["local", "hpc"]
    }
  },
  "spec": {
    "inputs": {
      "sumValue": {
        "type:integer": {
          "min": 0,
          "max": 30,
          "default": 1
        }
      },
      "filterValue": {
        "type:integer": {
          "min": 0,
          "max": 10,
          "default": 1
        }
      },
      "minFilterValue": {
        "type:integer": {
          "min": 0,
          "max": 10,
          "default": 0
        }
      }
    },
    "outputs": {}
  },
  "ui": {
    "inputs": {
      "sumValue": {
        "index": 0,
        "label": "Sum value",
        "widget:slider": {
          "step": 1
        }
      },
      "filterValue": {
        "index": 1,
        "label": "Filter value",
        "widget:slider": {
          "step": 1
        }
      },
      "minFilterValue": {
        "index": 1,
        "label": "Minimum filter value",
        "widget:slider": {
          "step": 1
        }
      }
    },
    "outputs": {}
  }
}
